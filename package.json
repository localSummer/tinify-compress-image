{
  "publisher": "localSummer",
  "name": "tinify-compress-image",
  "displayName": "tinify-compress-image",
  "description": "图片压缩工具",
  "version": "0.0.4",
  "packageManager": "npm@10.5.0",
  "author": "localSummer <roamingcode@163.com>",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/localSummer/tinify-compress-image"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension/extension.js",
  "icon": "res/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "tinify.compressImages",
        "title": "Tinify Compress Images"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "tinify.compressImages",
          "group": "tinify"
        }
      ]
    },
    "configuration": {
      "title": "Tinify Compress Images",
      "properties": {
        "tinify.apiKey": {
          "type": "string",
          "description": "API Key for TinyPNG."
        }
      }
    }
  },
  "scripts": {
    "compile": "vite build",
    "compile:pack": "npm run compile && vsce package -o dist/tinify-compress-image.vsix --no-dependencies",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@tomjs/vite-plugin-vscode": "^2.5.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "1.82.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^2.31.1",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.19.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "dependencies": {
    "tinify": "^1.8.0"
  }
}
